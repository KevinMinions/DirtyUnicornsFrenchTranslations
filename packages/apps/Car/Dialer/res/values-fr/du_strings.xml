<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Name of the phone dialer application [CHAR LIMIT=30]-->
    <string name="phone_app_name">Téléphone</string>
    <!-- Full-screen error text for when the phone can not be used because Bluetooth isn't connected [CHAR LIMIT=NONE]-->
    <string name="no_hfp">Pour passer ou recevoir des appels, connectez votre téléphone à votre voiture via le Bluetooth.</string>
